+#!/usr/bin/env python3
+"""
+Simple Milvus stress tester.
+ - Inserts synthetic vectors and runs concurrent search queries to exercise HA and latency.
+ - Usage:
+     python scripts/milvus_stress_test.py --host <MILVUS_HOST> --port 19530 --collection aegis_docs --inserts 1000 --queries 200 --concurrency 8
+"""
+import argparse
+import time
+import random
+import numpy as np
+from concurrent.futures import ThreadPoolExecutor
+
+def rand_vec(dim=128):
+    return np.random.rand(dim).astype(np.float32).tolist()
+
+def do_insert(client, collection, n, dim):
+    # client is a pymilvus Collection instance
+    docs = [rand_vec(dim) for _ in range(n)]
+    try:
+        client.insert([docs])
+        return True
+    except Exception as e:
+        print("Insert failed:", e)
+        return False
+
+def do_search(client, collection, qvec, topk=4):
+    try:
+        res = client.search([qvec], "embedding", param={"metric_type":"L2","params":{"nprobe":10}}, limit=topk)
+        return len(res[0])
+    except Exception as e:
+        print("Search failed:", e)
+        return 0
+
+def main():
+    p = argparse.ArgumentParser()
+    p.add_argument("--host", default="127.0.0.1")
+    p.add_argument("--port", default="19530")
+    p.add_argument("--collection", default="aegis_docs")
+    p.add_argument("--inserts", type=int, default=500)
+    p.add_argument("--queries", type=int, default=200)
+    p.add_argument("--dim", type=int, default=128)
+    p.add_argument("--concurrency", type=int, default=4)
+    args = p.parse_args()
+
+    try:
+        from pymilvus import connections, Collection, utility
+    except Exception:
+        print("pymilvus not installed. pip install pymilvus")
+        return
+
+    connections.connect(host=args.host, port=args.port)
+    if not utility.has_collection(args.collection):
+        print("Collection not found:", args.collection)
+        return
+    col = Collection(args.collection)
+
+    print("Starting inserts and queries...")
+    start = time.time()
+    with ThreadPoolExecutor(max_workers=args.concurrency) as ex:
+        # mix inserts and queries
+        futures = []
+        for _ in range(args.inserts // 50):
+            futures.append(ex.submit(do_insert, col, args.collection, 50, args.dim))
+        for _ in range(args.queries):
+            q = rand_vec(args.dim)
+            futures.append(ex.submit(do_search, col, q, 4))
+        results = [f.result() for f in futures]
+    dur = time.time() - start
+    print(f"Completed stress test in {dur:.2f}s: total ops {len(results)}")
+
+if __name__ == "__main__":
+    main()
+
