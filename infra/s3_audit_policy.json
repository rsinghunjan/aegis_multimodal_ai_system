{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowListBucket",
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::YOUR_AEGIS_AUDIT_BUCKET"
      ]
    },
    {
      "Sid": "AllowObjectWriteAndRead",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:GetObject",
        "s3:HeadObject",
        "s3:DeleteObject"
      ],
      "Resource": [
        "arn:aws:s3:::YOUR_AEGIS_AUDIT_BUCKET/*"
      ]
    },
    {
      "Sid": "AllowKMSForAudit",
      "Effect": "Allow",
      "Action": [
        "kms:GenerateDataKey",
        "kms:Encrypt",
        "kms:Decrypt"
      ],
      "Resource": [
        "arn:aws:kms:YOUR_REGION:YOUR_ACCOUNT:key/YOUR_KMS_KEY_ID"
      ]
    }
  ]
}
